//my solutions for https://learn.freecodecamp.org/javascript-algorithms-and-data-structures/javascript-algorithms-and-data-structures-projects/telephone-number-validator

function telephoneCheck(str) {
  let regex = /^1?-?\s?\(\d{3}\)-?\s?\d{3}-?\s?\d{4}$|^1?-?\s?\d{3}-?\s?\d{3}-?\s?\d{4}$/;
  return regex.test(str);
}
